	<script type="text/javascript">
		const ham = document.querySelector('#ham');
		const mobile_nav = document.querySelector('.nav_lists');
		const pricesSection = document.querySelector('.prices');

		ham.addEventListener('click', (e) => {
			e.preventDefault();
			mobile_nav.classList.toggle('extend');
		});


		pricesSection.addEventListener('click', (e) => {
			if(e.target.tagName = 'a') {
				// two input that has same name attribute in the page but as it appear top of the page it will be selected .
				const input = document.querySelector('input[name=page_name]');
				input.value = `Pricing page where "` +  e.target.parentElement.children[0].textContent +  `" is the header text` ;
			}
		})

	</script>
