<!-- signup popup modal -->
<div id="id01" class="modal">
  <form class="modal-content" action="/pricing" method="POST">
	<!-- <form class="modal-content" action="mailto:FFSale@arvogp.com" method="POST" enctype="multipart/form-data"> -->

    <div class="container">
      <h1 class="popup-heading">Sign Up</h1>
      <label for="name" hidden><b>Name</b></label>
      <input type="text" placeholder="Username" name="name" required>
      
      <label for="phone" hidden><b>Phone</b></label>
      <input type="tel" placeholder="Phone #" name="phone" required>
      
      <label for="email" hidden><b>Email</b></label>
      <input type="email" placeholder="Email" name="email" required>

      <label for="city" hidden><b>City</b></label>
      <input type="text" placeholder="City" name="city" required>

      <label for="state" hidden><b>State</b></label>
      <input type="text" placeholder="State" name="state" required>

      <div class="clearfix">
        <button type="button" onclick="document.getElementById('id01').style.display='none'" class="cancelbtn">Cancel</button>
        <button type="submit" class="signupbtn">Sign Up</button>
      </div>
    </div>
  </form>
</div>


