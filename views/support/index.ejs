<body>
	<%- include ("../partials/myHeader") %>
	<%- include ("../partials/modal") %>
	<%- include ("../partials/about") %>
	<section class="support">
		<div class="container-fluid support_parent">
			<div class="row">
				<div class="col-md-6 col-sm-12">
					<div class="contact_method">
						<i class="fas fa-phone"></i>
						<div>
							<h6>Text Or Call Us At</h6>
							<a href="callto:(833) 462-2786">(833) 462-2786</a>
						</div>
					</div>
					<div class="contact_method">
						<i class="fas fa-envelope"></i>
						<div>
							<h6>Email Us At</h6>
							<a href="mailto:FF@arvogp.com">FF@ArvoGP.com</a>
						</div>
					</div>
					<div class="contact_method">
						<i class="fas fa-sms"></i>
						<div>
							<h6>Chat With Us By</h6>
							<a href="https://www.tidio.com/talk/oohi4ck9zmzjoekpsft3cp6h9cnitwej" target="_blank">		   Clicking Here
							</a>
						</div>
					</div>
				</div>
				<div class="col-md-6 col-sm-12 form_container">
					<h6>Or Use This Form For Us To Reach You:</h6>
					<form action="/support" method="POST">
						<input type="text" name="name" placeholder="Your Full Name" required>
						<br>
						<input type="text" name="phone" placeholder="Your Phone Number" required>
						<br>
						<input type="text" name="email" placeholder="Your Email" required>
						<br>
						<input type="text" name="text" id="text" placeholder="Additional Info">
						<br>
            <input type="text" name="page_name" value="Support Page" style="display:none" >
						<button class="btn btn-success support_big_button_link" type="submit">
							Send
						</button>			
					</form>
				</div>
			</div>
			<div id="social_media_sources_support">
				<a href="https://www.facebook.com/FriendFoneApp" target="_blank">
					<i class="fab fa-facebook"></i>
				</a>
				<a href=" https://twitter.com/friend_fone" target="_blank"> 
					<i class="fab fa-twitter"></i>
				</a>
				<a href="https://www.instagram.com/friend_fone/" target="_blank">
					<i class="fab fa-instagram"></i>
				</a>
				<a href="https://www.tiktok.com/@friendfone.com" target="_blank">
					<i class="fab fa-tiktok" target="_blank"></i>
				</a>
			</div>
		</div>
	</section>
	<%- include ("../partials/about") %>
	<%- include ("../partials/newFooter") %>
	<script type="text/javascript">
		const ham = document.querySelector('#ham');
		const mobile_nav = document.querySelector('.nav_lists');

		ham.addEventListener('click', (e) => {
			e.preventDefault();
			mobile_nav.classList.toggle('extend');
		});

		const customerPhoto = document.querySelector(`section.about_section:nth-child(3) > div:nth-child(1) > div:nth-child(1) > div:nth-child(2) > div:nth-child(1)`);

		const headerSelector = `section.about_section:nth-child(3) .about_part_one div:nth-child(2) h1`;
		const paraSelector = `section.about_section:nth-child(3) .about_part_one div:nth-child(2) p`;
		const btnSelector = `section.about_section:nth-child(3) .about_part_one div:nth-child(2) a`
		document.addEventListener("DOMContentLoaded", (e) => {
			document.querySelector(headerSelector).textContent = `Stellar Support`;
			document.querySelector(paraSelector).textContent = `Our customer support team is here is to help with any questions you may have! Regardless of the question, we have an expert able to answer any questions. We offer 24-7 support and sales team access for FriendFone users. Never have trouble getting help!`;
			const btn = document.querySelector(btnSelector);
			btn.textContent = 'Contact Us';
			btn.target = '_blank';
			btn.href = 'https://www.tidio.com/talk/oohi4ck9zmzjoekpsft3cp6h9cnitwej';
			customerPhoto.style.backgroundImage = `url('https://mediaanduserfiles.s3.amazonaws.com/Customer_Service_Photo_j5a7vq.png')`;
		})

		// const iframe = document.querySelector('iframe');
		// iframe.src = `https://www.youtube.com/embed/aKHqlSTDop8`;

		const modalButton = document.querySelector(`section.about_section:nth-child(3) .about_part_two a `);
		const modalButtonTwo = document.querySelector(`section.about_section:nth-child(5) .about_part_two a`);
		
		modalButton.addEventListener('click', () => {
			//replace it with a new video
			const videoLink = `https://www.youtube.com/embed/aKHqlSTDop8`;
			const iframe = document.querySelector('iframe');
			// const source = document.querySelector('video source');
			if(iframe.src == videoLink){
				return;
			}

			iframe.src = videoLink;
		})

		modalButtonTwo.addEventListener('click', () => {
			const videoLink = `https://www.youtube.com/embed/TbI6XDbZFl8`;
			const iframe = document.querySelector('iframe');
			if(iframe.src == videoLink){
				return;
			}

			iframe.src = videoLink;
		})
	</script>
</body>
</html>